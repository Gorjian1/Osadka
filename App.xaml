<Application x:Class="Osadka.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:Osadka.Converters">

    <Application.Resources>

        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <conv:PositiveToVisibleConverter x:Key="PositiveToVisible"/>
        <conv:ZeroToVisibleConverter     x:Key="ZeroToVisible"/>
        <conv:JoinConverter x:Key="Join"/>
        <conv:RelaxedDoubleConverter x:Key="RelaxedDouble"/>
        
    </Application.Resources>
</Application>
